<Page x:Class="QQ112.Pages.BxodPages"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:QQ112.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="BxodPages">

    <Grid>
        <TextBox x:Name="LoginBox" Width="150" Height="20" Margin="0,0,180,0"/>
        <TextBox x:Name="PasswordBox" Width="150" Height="20" Margin="180,0,0,0"/>
        <Button x:Name="BxodBut" Width="100" Height="20" Margin="0,100,0,0" Click="BxodBut_Click"/>
    </Grid>
</Page>
